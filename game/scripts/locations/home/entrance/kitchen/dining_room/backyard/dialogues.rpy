label mom_finger_loop:
    show screen sex_anim_buttons
    pause
    hide screen sex_anim_buttons
    hide player finger
    $ animcounter = 0
    while animcounter < 4:
        if anim_toggle:
            if not animated:
                if M_mom.is_set("sex flip"):
                    show expression AnimatedImage("debbie", ["194f","195f","196f"], M_mom) as debbie at left
                    if M_mom.is_set("robe on"):
                        show expression AnimatedImage("debbie_robe", ["194bf","195bf","196bf"], M_mom) as debbie_robe at left
                else:

                    show expression AnimatedImage("debbie", [194,195,196], M_mom) as debbie at right
                    if M_mom.is_set("robe on"):
                        show expression AnimatedImage("debbie_robe", ["194b","195b","196b"], M_mom) as debbie_robe at right
            $ animated = True
            pause 4
            call expression game.dialog_select("debbie_finger_hscene_dialog")
            pause 3
        else:

            $ pose_counter = 0
            $ pose_list = [194,195,196]
            $ poses_done = []
            while poses_done != pose_list:
                if M_mom.is_set("sex flip"):
                    show expression "debbie {}f".format(pose_list[pose_counter]) as debbie at left
                else:
                    show expression "debbie {}".format(pose_list[pose_counter]) as debbie at right

                if M_mom.is_set("robe on"):
                    if M_mom.is_set("sex flip"):
                        show expression "debbie_robe {}bf".format(pose_list[pose_counter]) as debbie_robe at left
                    else:
                        show expression "debbie_robe {}b".format(pose_list[pose_counter]) as debbie_robe at right
                pause
                $ poses_done.append(pose_list[pose_counter])
                $ pose_counter += 1
            call expression game.dialog_select("debbie_finger_hscene_dialog")
        $ animcounter += 1
    call screen mom_finger_options

